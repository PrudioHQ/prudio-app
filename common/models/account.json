{
  "name": "Account",
  "plural": "accounts",
  "base": "PersistedModel",
  "idInjection": true,
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "createdAt": {
      "type": "date"
    },
    "lastModifiedAt": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "externalProviderTokens": {
      "type": "hasMany",
      "model": "externalProviderToken",
      "foreignKey": ""
    },
    "users": {
      "type": "hasAndBelongsToMany",
      "model": "user",
      "foreignKey": ""
    },
    "apps": {
      "type": "hasMany",
      "model": "App",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "teamMember",
      "permission": "ALLOW"
    }
  ],
  "methods": []
}
